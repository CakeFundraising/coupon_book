= f.semantic_fields_for :community, f.object.community || f.object.build_community do |c|
  = c.input :slug, prepend: community_page_root_url
  = c.input :affiliate_commission_percentage, as: :select, collection: commission_rate_options(c.object.affiliate_commission_percentage)
  /= c.input :media_commission_percentage, as: :select, collection: commission_rate_options(c.object.media_commission_percentage)
